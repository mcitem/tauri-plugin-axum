{
  "compilerOptions": {
    "target": "es2021",
    "module": "esnext",
    "moduleResolution": "bundler",

    "strict": true,
    "noEmit": true,
    "skipLibCheck": true,
    "noUnusedLocals": true,
    "noImplicitAny": true
  },
  "include": ["src/*.ts"],
  "exclude": ["dist-js", "node_modules"]
}
