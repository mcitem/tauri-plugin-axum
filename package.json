{
  "name": "@mcitem/tauri-plugin-axum",
  "version": "0.3.0",
  "author": "mcitem",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/mcitem/tauri-plugin-axum.git"
  },
  "description": "A Tauri plugin that allows calling Axum Router endpoints directly.",
  "keywords": [
    "tauri-plugin",
    "axum"
  ],
  "type": "module",
  "types": "./dist/index.d.ts",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./*": {
      "types": "./dist/*.d.ts",
      "import": "./dist/*.js",
      "require": "./dist/*.cjs"
    }
  },
  "files": [
    "dist",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "prepack": "rollup -c && shx rm README.md && shx cp tauri-plugin-axum/README.md ./README.md",
    "postpack": "shx rm README.md && shx ln -s tauri-plugin-axum/README.md ./README.md"
  },
  "dependencies": {
    "@tauri-apps/api": "^2"
  },
  "peerDependencies": {
    "axios": "^1"
  },
  "peerDependenciesMeta": {
    "axios": {
      "optional": true
    }
  },
  "devDependencies": {
    "@rollup/plugin-alias": "^5.1.1",
    "@rollup/plugin-typescript": "^12.1.4",
    "eslint": "^9.33.0",
    "@eslint/js": "^9.33.0",
    "globals": "^16.3.0",
    "rollup": "^4.46.2",
    "rollup-plugin-copy": "^3.5.0",
    "shx": "^0.4.0",
    "tslib": "^2.8.1",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.39.1"
  }
}
