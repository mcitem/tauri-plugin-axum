{
  "name": "@mcitem/tauri-plugin-axum",
  "version": "0.1.1",
  "author": "mcitem",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/mcitem/tauri-plugin-axum.git"
  },
  "description": "A Tauri plugin that allows calling Axum Router endpoints directly.",
  "keywords": [
    "tauri-plugin",
    "axum"
  ],
  "type": "module",
  "types": "./dist/index.d.ts",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "exports": {
    "types": "./dist/index.d.ts",
    "import": "./dist/index.js",
    "require": "./dist/index.cjs"
  },
  "files": [
    "dist",
    "LICENSE",
    "README"
  ],
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "prepack": "rollup -c && shx cp tauri-plugin-axum/README.md ./README",
    "postpack": "shx rm README"
  },
  "dependencies": {
    "@tauri-apps/api": "^2.7.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.33.0",
    "@rollup/plugin-typescript": "^12.1.4",
    "eslint": "^9.33.0",
    "globals": "^16.3.0",
    "rollup": "^4.46.2",
    "shx": "^0.4.0",
    "tslib": "^2.8.1",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.39.1"
  }
}